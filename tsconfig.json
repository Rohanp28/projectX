{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/reels.jsx",
    "app/(tabs)/search.jsx",
    "app/(tabs)/profile.jsx"
, "app/(tabs)/(profile)/_layout.jsx"  ]
}
